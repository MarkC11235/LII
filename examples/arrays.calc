NUM = 100000
array a[NUM] = {0}
array b[NUM] = {0}
array c[NUM] = {0}
array out[NUM] = {0}

for(i = 0; i < NUM; i = i + 1){
    array a[i] = i
    array b[i] = i * 5
    array c[i] = i * 3 / 2
}

for(i = 0; i < NUM; i = i + 1){
    array out[i] = a[i] + b[i] + c[i] 
}

for(i = 0; i < NUM; i = i + 1){
    out[i]
}
