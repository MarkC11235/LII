let factorial = func (n){
    let res = 1;
    for (let i = 1; i <= n; i = i + 1){
        res = res * i;
    }
    return res;
};

let sum_vector = func (v){
    let res = 0;
    for (let i = 0; i < $vector_len(v); i = i + 1){
        res = res + v[i];
    }
    return res;
};

let dot_product = func (v1, v2){
    let res = 0;
    for (let i = 0; i < $vector_len(v1); i = i + 1){
        res = res + v1[i] * v2[i];
    }
    return res;
};

